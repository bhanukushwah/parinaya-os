{
	"name": "parinaya-os",
	"private": true,
	"workspaces": {
		"packages": [
			"apps/*",
			"packages/*"
		],
		"catalog": {
			"dotenv": "^17.2.2",
			"csv-parse": "^6.1.0",
			"libphonenumber-js": "^1.12.25",
			"zod": "^4.1.13",
			"typescript": "^5",
			"@types/bun": "^1.3.4",
			"elysia": "^1.4.21",
			"@orpc/server": "^1.12.2",
			"@orpc/openapi": "^1.12.2",
			"@orpc/zod": "^1.12.2",
			"better-auth": "^1.4.18",
			"@orpc/client": "^1.12.2"
		}
	},
	"type": "module",
	"scripts": {
		"dev": "turbo dev",
		"build": "turbo build",
		"check-types": "turbo check-types",
		"dev:native": "turbo -F native dev",
		"dev:web": "turbo -F web dev",
		"dev:server": "turbo -F server dev",
		"db:push": "turbo -F @parinaya-os/db db:push",
		"db:studio": "turbo -F @parinaya-os/db db:studio",
		"db:generate": "turbo -F @parinaya-os/db db:generate",
		"db:migrate": "turbo -F @parinaya-os/db db:migrate",
		"check": "biome check --write ."
	},
	"dependencies": {
		"@parinaya-os/env": "workspace:*",
		"dotenv": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@parinaya-os/config": "workspace:*",
		"@types/bun": "catalog:",
		"turbo": "^2.6.3",
		"typescript": "catalog:"
	},
	"packageManager": "bun@1.2.15"
}
